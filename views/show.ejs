<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <% include css %>
    <title>Hello, world!</title>
</head>
<body class="container">
<%- include nav %>
<h1><%=title%></h1>
<!--class="col-5 col-sm-4"-->
<div class="row">

    <% for (var i = 0; i < dataLab5.length; i++) { %>
        <div class="col-5 col-sm-4">

            <div style="width: 400px;height:300px ;background-image: url( '<%= dataLab5[i].linkPhoto %>') ;background-size:cover; ">
                <form style="margin-left: 360px" method="post" action="/delete"
                      enctype="application/x-www-form-urlencoded">
                    <input name="idCar" hidden value="<%= dataLab5[i].id %>">
                    <button type="submit">Xóa</button>
                </form>


                <form style="margin-left: 360px" method="post" action="/update-car"
                      enctype="application/x-www-form-urlencoded">
                    <input name="idCar" hidden value="<%= dataLab5[i].id %>">
                    <input hidden name="name" value="<%= dataLab5[i].name %>" >
                    <input hidden name="content" value="<%= dataLab5[i].content %>" >
                    <input hidden name="linkPhoto" value="<%= dataLab5[i].linkPhoto %>">
                    <button type="submit">Sửa</button>
                </form>

            </div>
            <p><%= dataLab5[i].name %></p>
        </div>
    <% } %>

</div>


<!-- Option 1: Bootstrap Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>

