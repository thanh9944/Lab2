<!doctype html>
<html lang="en">
<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <% include css %>
  <title>Hello, world!</title>
</head>
<body class="container">
<%-include nav%>
<div class="row">
  <form  class="col" method="post" action="/lab5-sua" enctype="application/x-www-form-urlencoded">
    <div class="row" style="width: 500px">

      <p style="font-size: 20px">Name photo</p>
      <input  name="name" placeholder="Enter name's photo" value="<%=namecar%>">
      <p style="font-size: 20px;margin-top: 10px">Content</p>
      <input  name="content" placeholder="Enter content" value="<%=content%>">
      <p style="font-size: 20px;margin-top: 10px">Link photo</p>
      <input  name="link"  placeholder="Enter link's photo" value="<%=linkPhoto%>">
      <input  hidden name="idCar" value="<%=id%>">
      <button style="margin-top: 20px" type="submit">Update</button>

    </div>
  </form>
</div>


