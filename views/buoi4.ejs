<!doctype html>
<html lang="en">
<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <% include css %>
  <title>Hello, world!</title>
</head>
<body class="container">
<%-include nav%>
<h1> <%=title%> </h1>
<h1> <%=name%> </h1>
<h1> <%=array%> </h1>
<h1> <%=sinhvien.name%> </h1>
<h1> <%=sinhvien.msv%> </h1>
<!-- Optional JavaScript; choose one of the two! -->
<h1>
<% for (var i = 0; i < array.length; i++) { %>
  <div class="btn btn-primary"> <%= array[i] %> </div>
<% } %>
</h1>
<h1>
  Danh sach
</h1>
<h1><%=typeof danhsach%></h1>
<% for (var i = 0; i < danhsach.length; i++) { %>
  <div class="btn btn-primary"> <%= danhsach[i].name %> </div>
  <div class="btn btn-primary"> <%= danhsach[i].msv %> </div>
<% } %>
<h1>
  Thong tin
</h1>
<p><%=thongTin.name%></p>
<p><%=thongTin.sdt%></p>
<% for (var i = 0; i < thongTin.danhsach.length; i++) { %>
  <div class="btn btn-primary"> <%= thongTin.danhsach[i].name %> </div>
  <div class="btn btn-primary"> <%= thongTin.danhsach[i].msv %> </div>
<% } %>

<!-- Option 1: Bootstrap Bundle with Popper -->
